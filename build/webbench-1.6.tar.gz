	<script type="text/javascript">
		
		TS_BAR_CONF = {
			reloadPage	: 0,
			service		: "tiscali",
			checkOldSession : 0,
			
				URL_LOGIN	: "//login.tiscali.cz",
				showInbox	: 1,
				callback : function(out) {
					$(function() {
						try {
							callbackHP(out)
						}
						catch(err){ console.log(err) }
					})
				},
			
			'':''
		}

		
		function callbackHP(out) {
		    if (out.status!=200 || out.data.ebox==0) {
			$("#hp-email-login").show()
			$("#hp-email-message").hide()
			return
		    }
		    else {
			$("#hp-email-login").hide()
			$("#hp-email-message").show()
		    }
		    if (out.data.cntNewMail==0) {
			$("#email-unread-msg").text('žádné nové emaily');
		    }
		    if (out.data.messageList) {
			out.data.messageList.forEach(function(item) {
			    var li = $('<li '+(item.read=='unread'?'class="bold"':'')+'><a href="http://email.tiscali.cz/messageDetail?messageId='+item.id+'"><span class="time"></span> <span class="from"></span><span class="subject"></span></a></li>')
			    li.find(".subject").text(item.subject)
			    li.find(".from").text(item.emailFromName || item.emailFrom)
			    var date = new Date(item.timeCreate*1000);
			    if (date.toDateString() == new Date().toDateString()) {
				var m = date.getMinutes()
				if (m<10) { m = '0'+m }
				li.find(".time").text(date.getHours()+":"+m)
			    }
			    else {
				li.find(".time").text(date.getDate()+"."+(date.getMonth()+1)+".")
			    }
			    $("ul.messages").append(li)
			    //alert(item.subject)
			} )
		    }
		}
		
	</script>


