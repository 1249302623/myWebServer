<script>
    function showGdpr(e) {
        e.preventDefault();

        $('.mobile-login-form').hide();
        $('.gdpr-lightbox').show();
        $('.gdpr-lightbox .loading-spinner').show();
        $.post("https://login.tiscali.cz/ajax/gdpr-check", $('#loginForm').serialize())
            .done(function(resolved) {
                $('.gdpr-lightbox .loading-spinner').hide();
                if (resolved) {
                    submitGdpr();
                    return;
                }

                $('#btn-gdpr-next').one('click', showGdprSubmit);
                $('#gdpr-general-conditions').show();
            })
            .fail(function(err) {
                $('.gdpr-lightbox').hide();
                // nechame chybu vyprintit server
                submitGdpr();
            });
    }

    function showGdprSubmit(e) {
        e.preventDefault();
        e.stopPropagation();
        $("#gdpr").val(1);
        $('#btn-gdpr-submit').one('click', submitGdpr);
        $('#gdpr-general-conditions, #gdpr-direct-mails').toggle();
    }

    function submitGdpr(e) {
        if (e) {
            e.preventDefault();
            e.stopPropagation();
        }

        $('#loginForm').off('submit', showGdpr);
        $('#loginForm').submit();
    }

    $('#loginForm').on('submit', showGdpr);
</script>
